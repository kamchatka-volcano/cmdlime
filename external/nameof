include(FetchContent)
Set(FETCHCONTENT_QUIET FALSE)
if (NOT nameof_FOUND)
    FetchContent_Declare(
            nameof
            GIT_REPOSITORY https://github.com/Neargye/nameof.git
            GIT_TAG        v0.10.1
            GIT_SHALLOW    ON
            GIT_PROGRESS TRUE
    )
    FetchContent_MakeAvailable(nameof)
endif()
